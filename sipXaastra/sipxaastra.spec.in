Name: @PACKAGE@
Version: @VERSION@
Release: @PACKAGE_REVISION@

Summary: VoIP PBX 
License: LGPL
Group: Telcommunications
Vendor: SIPfoundry
Packager: SIPfoundry <sipx-dev@sipfoundry.org>
Url: http://www.sipfoundry.org

%if %{_vendor} == suse
BuildRequires: java-1_6_0-sun-devel
%else
BuildRequires: java-1.7.0-openjdk-devel
%endif
BuildRequires: automake
BuildRequires: sipxcommons >= %version
BuildRequires: sipxconfig >= %version
BuildRequires: sipxhotprovisioning

Requires: sipxconfig
Requires: sipxhotprovisioning

Source: %name-%version.tar.gz

Prefix: %_prefix
BuildRoot: %{_tmppath}/%name-%version-root

%description
Configuration support aastra phones

%prep
%setup -q

%build
%configure
make

%install
rm -rf $RPM_BUILD_ROOT
make DESTDIR=$RPM_BUILD_ROOT install

%clean
rm -rf $RPM_BUILD_ROOT

%files
%defattr(644,sipx,sipx,755)
%attr(755,root,root) %{_sysconfdir}/sipxpbx/aastra/*
%{_datadir}/java/sipXecs/sipXconfig/plugins/sipXaastra.jar



